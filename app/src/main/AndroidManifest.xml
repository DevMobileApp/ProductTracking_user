        android:theme="@style/AppTheme">
        <activity android:name=".pin_pattern_password"></activity>
        <activity android:name=".finger_print_password" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Product_item_monitor">
